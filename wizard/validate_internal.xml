<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="wizard_validate_internal_transfer_inventoryadjustment_form_readonly" model="ir.ui.view">
            <field name="name">wizard.inventory.adjustment.form.readonly</field>
            <field name="model">wizard.inventory.adjustment</field>
            <field name="arch" type="xml">
                <form string="Wizard">
                    <group>
                        <field name="inventory_date"/>
                        <field name="inventory_remark"/>
                    </group>
                <footer> 
                    <button name="custom_backdateorder_button_inventoryadjust" type="object" string="Process Backdate" class="oe_highlight"/> or <button name="cancel" string="Cancel" special="cancel" class="oe_link"/> 
                </footer>
                </form>
            </field>
        </record>

<record id="wizard_validate_internal_transfer_inventoryadjustment_action_window" model="ir.actions.act_window">
        <field name="name">Process Transfer of Inventory</field>
        <field name="res_model">wizard.inventory.adjustment</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
</record>

</odoo>